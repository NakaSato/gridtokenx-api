  "query": "\n            SELECT \n                id, epoch_id, buyer_id, seller_id, energy_amount,\n                price_per_kwh, total_amount, fee_amount, net_amount, status\n            FROM settlements \n            WHERE buyer_id = $1 OR seller_id = $1\n            ORDER BY created_at DESC\n            LIMIT $2 OFFSET $3\n            ",
